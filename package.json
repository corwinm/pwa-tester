{
  "name": "pwa-tester",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://corwinm.github.io/pwa-tester",
  "dependencies": {
    "bulma": "^0.7.4",
    "cra-append-sw": "^2.6.1",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.11.0",
    "preval.macro": "^3.0.0",
    "react": "^16.8.6",
